            <validation>String disallowed = (System.getProperty("user.home") + File.separator + context.getCompilerVariable("sys.shortName"));
if (Util.isMacOS()) {
    disallowed = (System.getProperty("user.home") + File.separator + "Documents" + File.separator + context.getCompilerVariable("sys.shortName"));
}

File disallowed_file = new File(disallowed);

String mcv_jar =disallowed + File.separator + "mcidasv.jar";
File mcv_jar_file = new File(mcv_jar);

String mcv_main =disallowed + File.separator + "main.xml";
File mcv_main_file = new File(mcv_main);

Boolean allowed = true;

if (context.getInstallationDirectory().getPath().equals(disallowed)) {
    String notice = disallowed + " is not an allowed installation location.\n\n";
    notice += "Starting with 1.0beta6 user data is stored here.\nPlease select a different installation location.";
    Util.showErrorMessage(notice);
    allowed = false;
}
else if (disallowed_file.exists()) {
    if (mcv_jar_file.exists()) {
        String notice = disallowed + " contains a previous McIDAS-V installation.\n\n";
        notice += "Starting with 1.0beta6 user data is stored here.\nPlease uninstall McIDAS-V from this location before continuing.";
        Util.showErrorMessage(notice);
        allowed = false;
    }
    else if (!mcv_main_file.exists()) {
        String notice = disallowed + " already exists.\n\n";
        notice += "Starting with 1.0beta6 user data is stored here.\nPlease rename or remove this directory before continuing.";
        Util.showErrorMessage(notice);
        allowed = false;
    }
}

return allowed;</validation>
