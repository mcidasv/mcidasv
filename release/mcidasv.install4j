            </serializedBean>
            <condition>import java.io.*;
import java.lang.*;

if (!(Util.isMacOS())) return false;

float ver = 1.0f;
try {
    Process p = Runtime.getRuntime().exec("/usr/bin/java -version");
    p.waitFor();
    BufferedReader err = new BufferedReader(
        new InputStreamReader(p.getErrorStream()));
    String s = err.readLine();
    int idx = s.indexOf('"') + 1;
    s = s.substring(idx, idx+3);
    ver = Float.valueOf(s);
}
catch (Exception e) {}
return ver &lt; 1.6;</condition>
